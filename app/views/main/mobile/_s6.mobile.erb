<div id="section6" class="artist">
	<%= image_tag("mobile/section6/bg.jpg", class: "fucking_ie_bg", alt: "") if fucking_ie? %>
			
	<div class="intro">
		<%= image_tag "mobile/section6/intro.png" %>
	</div>

	<div class="s6lp popups" style="display:none">
		<%= image_tag "mobile/section6/btn_left_popup.png", class: "left" %>
		<div class="close"></div>
	</div>
	<div class="s6rp popups" style="display:none">
		<%= image_tag "mobile/section6/btn_right_popup.png", class: "right" %>
		<div class="close"></div>
	</div>


	<div class="wrapper">
		<div class="scene1">
			<%= image_tag "mobile/section6/top.png", class: "top" %>
			<%= image_tag "mobile/section6/bottom.png", class: "bottom" %>
			<%= image_tag "mobile/section6/left.png", class: "left s6l" %>
			<%= image_tag "mobile/section6/right.png", class: "right s6r" %>
		</div>
		
		<div class="scene2">
			<div data-partial="masking">
				<%= image_tag "mobile/section4/scene/0.png" %>
			</div>
			<div class="movie">
				<%= image_tag "mobile/section2/movie.png" %>
			</div>
			<%= image_tag "mobile/section6/text2.png", class: "text2" %>
		</div>
	</div>
</div>

<script type="text/javascript" charset="utf-8">
	$(".s6l").click(function(e){
		ss.enableWindowScrollEvents(false);
		$(".s6lp").show();
	})
	$(".s6r").click(function(e){
		ss.enableWindowScrollEvents(false);
		$(".s6rp").show();
	})
</script>