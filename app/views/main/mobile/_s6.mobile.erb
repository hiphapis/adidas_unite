<div id="section6" class="artist">
	<%= image_tag("mobile/section6/bg.jpg", class: "fucking_ie_bg", alt: "") if fucking_ie? %>
			
	<div class="intro">
		<%= image_tag "mobile/section6/intro.png" %>
	</div>

	<div class="s6tp popups" style="display:none">
		<%= image_tag "mobile/section6/top_popup.png", class: "top" %>
	</div>
	<div class="s6bp popups" style="display:none">
		<%= image_tag "mobile/section6/bottom_popup.png", class: "bottom" %>
	</div>
	<div class="s6lp popups" style="display:none">
		<%= image_tag "mobile/section6/btn_left_popup.png", class: "left" %>
		<div class="close"></div>
	</div>
	<div class="s6rp popups" style="display:none">
		<%= image_tag "mobile/section6/btn_right_popup.png", class: "right" %>
		<div class="close"></div>
	</div>


	<div class="wrapper">
		<div class="scene1">
			<%= image_tag "mobile/section6/top.png", class: "top popup", data: { source: ".s6tp" } %>
			<%= image_tag "mobile/section6/bottom.png", class: "bottom popup", data: { source: ".s6bp" } %>
			<%= image_tag "mobile/section6/left.png", class: "left popup", data: { source: ".s6lp" } %>
			<%= image_tag "mobile/section6/right.png", class: "right popup", data: { source: ".s6rp" } %>
		</div>
		
		<div class="scene2">
			<div data-partial="masking">
				<%= image_tag "mobile/section4/scene/0.png" %>
			</div>
			<div class="movie">
				<%= image_tag "mobile/section2/movie.png" %>
			</div>
			<%= image_tag "mobile/section6/text2.png", class: "text2" %>
		</div>
	</div>
</div>